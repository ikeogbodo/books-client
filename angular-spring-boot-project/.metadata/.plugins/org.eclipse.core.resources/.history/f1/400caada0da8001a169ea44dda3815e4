package com.ike.books.repository;

import org.springframework.data.repository.Repository;

import com.ike.books.entity.Tele;

import java.util.List;

public interface TeleRepository extends Repository<Tele, Integer> {

    void delete(Tele user);

    List<Tele> findAll();

    Tele findOne(int id);

    Tele save(Tele user);
}
