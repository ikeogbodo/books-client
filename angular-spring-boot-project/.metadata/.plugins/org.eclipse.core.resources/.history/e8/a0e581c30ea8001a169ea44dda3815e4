package com.ike.books.entity;

import javax.persistence.*;

@Entity
@Table(name = "user")
public class Book {

    @Id
    @Column
    @GeneratedValue(strategy = GenerationType.IDENTITY)
    private int id;
    
    @Column
    private String title;
    
    @Column
    private String author;
    
    @Column
    private String publisher;
    
    @Column
    private String pages;
    
 
}
